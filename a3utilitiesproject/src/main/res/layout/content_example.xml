<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <CheckBox
        android:id="@+id/cbO"
        android:layout_width="190dp"
        android:layout_height="32dp"
        android:checked="false"
        android:enabled="false"
        android:text="isOxxOrAbove"/>

    <CheckBox
        android:id="@+id/cbKitkat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:enabled="false"
        android:text="isKitkatOrAbove"/>

    <CheckBox
        android:id="@+id/cbLollipop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:enabled="false"
        android:text="isLollipopOrAbove"/>

    <CheckBox
        android:id="@+id/cbMarshmallow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:enabled="false"
        android:text="isMarshmallowOrAbove"/>

    <CheckBox
        android:id="@+id/cbNougat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:checked="false"
        android:enabled="false"
        android:text="isNougatOrAbove"/>

</LinearLayout>
